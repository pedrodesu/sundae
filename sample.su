func swap(a &mut u32, b &mut u32) {
    c u32 := *a
    *a = *b // a is ptr, *a is i32, how to get original ptr back?
    *b = c
}

func op(a u32, b u32) u32 {
    ret a * b
}

func main() {
    a mut u32 := 10
    b mut u32 := 32

    swap(&a, &b)
    
    putd(a)
    putd(b)
    // not swapping ?
}

