func swap(a &mut i32, b &mut i32) {
    c i32 := a
    a = b
    b = c
}

// mut can be mutated, immutable by default
// const implies value at compile time (aka zig's comptime)

const MAGIC_NUMBER i32 = 42

func op(a i32, b i32) i32 {
    if a > b {
        ret a * b
    } else {
        ret a + b
    }
}

func main() {
    a mut i32 := op(4, 10)
    b i32
    
    b = MAGIC_NUMBER

    swap(a, b)

    putd(a)
    putd(b)
}
